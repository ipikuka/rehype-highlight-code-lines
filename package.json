{
  "name": "rehype-highlight-code-lines",
  "version": "1.2.0",
  "description": "Rehype plugin to add line numbers to code blocks and allow highlighting of desired code lines",
  "type": "module",
  "exports": "./dist/esm/index.js",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc --build && type-coverage",
    "format": "npm run prettier && npm run lint",
    "prettier": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest --watch=false",
    "test:watch": "vitest",
    "test:file1": "vitest test.md.spec.ts",
    "test:file2": "vitest test.html.spec.ts",
    "test:file3": "vitest test.cases.spec.ts",
    "test:file4": "vitest from-rehype-highlight/test.rhcl2.spec.ts",
    "test:file5": "vitest test.raw.spec.ts",
    "test:file6": "vitest test.debug.spec.ts",
    "prepack": "npm run build",
    "prepublishOnly": "npm run test && npm run format && npm run test-coverage",
    "test-coverage": "vitest run --coverage"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipikuka/rehype-highlight-code-lines.git"
  },
  "keywords": [
    "unified",
    "rehype",
    "hast",
    "markdown",
    "mdx",
    "plugin",
    "highlight",
    "highlighting",
    "rehype plugin",
    "rehype highlight",
    "code highlighting",
    "syntax highlighting",
    "line numbering",
    "line highlighting"
  ],
  "author": "ipikuka <talatkuyuk@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ipikuka/rehype-highlight-code-lines#readme",
  "bugs": {
    "url": "https://github.com/ipikuka/rehype-highlight-code-lines/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/dedent": "^0.7.2",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/eslint-plugin": "^1.4.2",
    "dedent": "^1.7.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rehype": "^13.0.2",
    "rehype-highlight": "^7.0.2",
    "rehype-parse": "^9.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "rimraf": "^6.1.0",
    "type-coverage": "^2.29.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "unified": "^11.0.5",
    "unist-util-remove-position": "^5.0.0",
    "vfile": "^6.0.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@types/hast": "^3.0.4",
    "parse-numeric-range": "^1.3.0",
    "unist-util-visit": "^5.0.0"
  },
  "peerDependencies": {
    "unified": "^11"
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "ignoreAsAssertion": true,
    "strict": true
  },
  "sideEffects": false
}
